{
  "name": "carshop",
  "version": "0.0.1",
  "description": "Business Application",
  "dependencies": {
    "@sap/cds": "^5",
    "@sap/audit-logging": "^3",
    "@sap/hana-client": "^2",
    "@sap/xsenv": "^3",
    "@sap/xssec": "^3",
    "passport": "^0.4.1",
    "debug": "^4.3",
    "express": "^4"
  },
  "devDependencies": {
    "@sap/ux-specification": "^1.84.7",
    "sqlite3": "5.0.0"
  },
  "scripts": {
    "start": "npx cds run"
  },
  "cds": {
    "requires": {
      "uaa": {
        "kind": "xsuaa"
      },
      "auth": {
        "[production]": {
          "strategy": "JWT"
        },
        "[development]": {
          "strategy": "mock"
        }
      },
      "db": {
        "kind": "sqlite",
        "model": "*",
        "credentials": {
          "database": "carshop.db"
        }
      }
    },
    "hana": {
      "deploy-format": "hdbtable"
    }
  },
  "sapux": [
    "app/admin",
    "app/browse"
  ]
}