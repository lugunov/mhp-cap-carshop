{
  "name": "carshop",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/cds": "^4",
    "express": "^4",
    "@sap/hana-client": "^2.6.61",
    "@sap/cds-odata-v2-adapter-proxy": "^1.7.6"
  },
  "devDependencies": {
    "@sap/ux-specification": "^1.90.13",
    "sqlite3": "5.0.0"
  },
  "scripts": {
    "start": "cds run",
    "build": "cds build",
    "deploy": "cds deploy --to sqlite:car.db",
    "undeploy": "cf undeploy carshop --delete-services --delete-service-keys"
  },
  "cds": {
    "hana": {
      "deploy-format": "hdbtable"
    },
    "requires": {
      "db": {
        "kind": "sqlite",
        "credentials": {
          "database": "car.db"
        }
      }
    }
  },
  "sapux": [
    "app/order-report"
  ]
}